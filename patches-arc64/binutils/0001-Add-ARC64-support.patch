+  lddl r2, [r0]                  ; lddl a, [b, s9=0]
+  lddl r6, [r7,255]              ; lddl a, [b, s9]    biggest s9
+  lddl r6, [r7,-256]             ; lddl a, [b, s9]    smallest s9
