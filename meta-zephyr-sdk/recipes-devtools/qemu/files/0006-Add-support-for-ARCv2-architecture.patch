+TCGv arc2_gen_next_reg(TCGv reg)
+{
+    int i;
+    for (i = 0; i < 64; i+= 2) {
+        if (reg == cpu_r[i]) {
+            return cpu_r[i+1];
+        }
+    }
+    g_assert_not_reached();
+
+    /* We never get here but to accommodate -Werror ... */
+    return NULL;
+}
