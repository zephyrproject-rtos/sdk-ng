+int arc_cpu_gdb_read_register(CPUState *cs, GByteArray *mem_buf, int n)
+{
+  ARCCPU *cpu = ARC_CPU(cs);
+  CPUARCState *env = &cpu->env;
+  uint32_t regval = 0;
+
+  switch (n)
+  {
+    case 0 ... 31:
+      regval = env->r[n];
+      break;
+    case GDB_REG_58:
+      regval = env->r[58];
+      break;
+    case GDB_REG_59:
+      regval = env->r[59];
+      break;
+    case GDB_REG_60:
+      regval = env->r[60];
+      break;
+    case GDB_REG_63:
+      regval = env->r[63];
+      break;
+    default:
+      assert(!"Unsupported register is being read.");
+  }
+
+  return gdb_get_reg32(mem_buf, regval);
+}
+
+
+int arc_cpu_gdb_write_register(CPUState *cs, uint8_t *mem_buf, int n)
+{
+  ARCCPU *cpu = ARC_CPU(cs);
+  CPUARCState *env = &cpu->env;
+  uint32_t regval = ldl_p(mem_buf);
+
+  switch (n)
+  {
+    case 0 ... 31:
+      env->r[n] = regval;
+      break;
+    case GDB_REG_58:
+      env->r[58] = regval;
+      break;
+    case GDB_REG_59:
+      env->r[59] = regval;
+      break;
+    case GDB_REG_60:
+      env->r[60] = regval;
+      break;
+    case GDB_REG_63:
+      env->r[63] = regval;
+      break;
+    default:
+      assert(!"Unsupported register is being written.");
+  }
+
+  return 4;
+}
